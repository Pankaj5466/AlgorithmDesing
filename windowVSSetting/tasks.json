{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: Build Current cpp file",
			"command": "C:/msys64/mingw64/bin/g++.exe",
			"args": [
				"-g",
				"${file}"
			],
			"options": {
				"cwd": "${fileDirname}"
